                    });
                    modalWrapperVideo.appendChild(playPauseVideo);

                    const videoPlayer = document.querySelectorAll('.storiesVideo');
                    videoPlayer.forEach((video) => {
                      video.pause();
                      video.addEventListener('click', () => {
                        console.log(modalContainerVideos);

                        let bigvideosrc = video.dataset.video;
                        let searchexist = modalContainerVideos.querySelector(`video[src="${bigvideosrc}"]`);
                        modalContainerVideos.querySelectorAll(`video`).forEach((bigvideos) => {
                          bigvideos.pause();
                          bigvideos.classList.add('hidden');
                        });
                        if (Boolean(searchexist)) {
                          searchexist.play();
                          searchexist.classList.remove('hidden');
                          modalWrapperVideo.classList.remove('hidden');
                        } else {
                          const bigVideo = document.createElement('video');
                          bigVideo.src = bigvideosrc;
                          bigVideo.loop = true;
                          modalContainerVideos.appendChild(bigVideo);
                          modalWrapperVideo.classList.remove('hidden');
                          bigVideo.play();
                        }
                      });
                      video.addEventListener('ended', playNextVideo);
                    });

                    let currentVideoIndex = 0;

                    function playNextVideo() {
                      currentVideoIndex++;
                      if (currentVideoIndex >= videoPlayer.length) {
                        currentVideoIndex = 0; // Loop back to the first video
                      }

                      let srcload = videoPlayer[currentVideoIndex].dataset.src;
                      videoPlayer[currentVideoIndex].src == '' ? (videoPlayer[currentVideoIndex].src = srcload) : false;
                      videoPlayer[currentVideoIndex].play();
                    }
                  });
                  function toggleBoxProduct(pelem) {
                    pelem.closest('.productWrapperModalVideo').classList.toggle('hideProductBoxModal');
                  }
                  function toggleSound(pelem) {
                    pelem.closest('.productWrapperModalVideo').classList.toggle('disableSound');
                    pelem.classList.toggle('disableSoundBtn');
                    pelem
                      .closest('.modalWrapperVideo')
                      .querySelectorAll('video')
                      .forEach((video) => {
                        if (pelem.classList.contains('disableSoundBtn')) {
                          video.muted = true;
                        } else {
                          video.muted = false;
                        }
                      });
                  }
                </script>
            {%- endcase -%}
          {%- endfor -%}
          <a href="{{ product.url }}" class="link product__view-details animate-arrow">
            {{ 'products.product.view_full_details' | t }}
            {% render 'icon-arrow' %}
          </a>
        </section>
